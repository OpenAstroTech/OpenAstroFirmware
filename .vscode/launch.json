{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Zephyr IDE: Debug",
            "cwd": "${workspaceFolder}",
            "gdbPath": "${command:zephyr-ide.get-gdb-path}",
            "executable": "${command:zephyr-ide.get-active-build-path}/zephyr/zephyr.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "interface": "swd",
            "configFiles": [
                "${command:zephyr-ide.get-active-build-board-path}/support/openocd.cfg"
            ]
        },
    ]
}